{
  "name": "astroindia-node-server",
  "version": "1.0.0",
  "description": "Node.js server for AstroIndia backend with Socket.IO for chat and call functionality",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "keywords": [
    "nodejs",
    "socket.io",
    "chat",
    "call",
    "astroindia"
  ],
  "author": "AstroIndia Team",
  "license": "MIT",
  "dependencies": {
    "socket.io": "^4.7.4",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
